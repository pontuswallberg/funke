{
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"beautify.options": {
		"indent_with_tabs": true,
		"end_with_newline": false
	},
	"html.format.endWithNewline": true,
	"emmet.syntaxProfiles": {
		"mustache": "html"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/docs": true,
		"**/funke": true,
		"**/css": true
	}
}
